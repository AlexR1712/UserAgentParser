{
    "name": "thadafinser/user-agent-parser",
    
    "description": "UserAgentParser abstraction for different parse providers",
    "keywords": [
        "UserAgentParser", 
        "UserAgent", 
        "Parser", 
        "Detection",
        "User agent",
        "UA",
        "Browser",
        "Rendering engine",
        "Operating system",
        "OS",
        "Device",
        
        "mobile detection", 
        "bot detection", 
        
        "browscap",
        "phpuseragentparser",
        "device detector",
        "whichbrowser",
        "woothee"
    ],
    
    "license": "MIT",
    "authors": [
        {
            "name": "ThaDafinser",
            "email": "martin.keckeis1@gmail.com"
        }
    ],
    
    "autoload": {
        "psr-4": {
            "UserAgentParser\\":  "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "UserAgentParserTest\\":  "tests/unit"
        }
    },
    
    "require": {
        "php": ">=5.5"
    },
    
    
    "suggest": {
        "browscap/browscap-php": "Needed to use Provider\BrowscapPhp",
         
        "donatj/phpuseragentparser": Needed to use Provider\DonatjUAParser",
        
        "piwik/device-detector": "Needed to use Provider\PiwikDeviceDetector",
        
        "sinergi/browser-detector": Needed to use Provider\SinergiBrowserDetector",
        
        "ua-parser/uap-php": "Needed to use Provider\UAParser",
        
        "whichbrowser/parser": "Needed to use Provider\WhichBrowser",
        
        "woothee/woothee": "Needed to use Provider\Woothee",
        
        "mimmi20/wurfl": "Needed to use Provider\Wurfl",
        
        "yzalis/ua-parser": "Needed to use Provider\YzalisUAParser"
    },
    
    "require-dev": {
        "phpunit/PHPUnit": "^4.0|^5.0",
        
        "fabpot/php-cs-fixer": "^1.10.0",
        
        "browscap/browscap-php": "dev-master",
         
        "donatj/phpuseragentparser": "^0.5.0",
        
        "piwik/device-detector": "^3.4",
        
        "sinergi/browser-detector": "^5.1",
        
        "ua-parser/uap-php": "^3.4",
        
        "whichbrowser/parser": ">=2.0.3,<2.1",
        
        "woothee/woothee": "^1.2",
        
        "mimmi20/wurfl": "^1.6",
        
        "yzalis/ua-parser": "^0.2"
    }
}
